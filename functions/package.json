{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase emulators:start --import=./saved-data --export-on-exit=./saved-data",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "index.js",
  "dependencies": {
    "@google-cloud/logging": "^9.7.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.19.0",
    "node-fetch": "^3.2.3",
    "request": "^2.88.2",
    "stripe": "^8.214.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
