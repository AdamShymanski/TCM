{
  "expo": {
    "userInterfaceStyle": "dark",
    "name": "PTCG Marketplace",
    "scheme": "ptcgm",
    "slug": "ptcg_marketplace",
    "version": "1.25.0",

    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1b1b1b"
    },
    "updates": {
      "fallbackToCacheTimeout": 300,
      "enabled": true,
      "checkAutomatically": "ON_LOAD"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.ptcg.marketplace"
    },
    "android": {
      "package": "app.ptcg.marketplace",

      "googleServicesFile": "./google-services.json",
      "userInterfaceStyle": "dark",
      "permissions": [],
      "versionCode": 1250,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-2637485113454186~8246765572"
      }
    },
    "plugins": [
      "sentry-expo",
      [
        "expo-media-library",
        {
          "photosPermission": "The app accesses your photos to let you upload photos of cards.",
          "savePhotosPermission": "The app accesses your photos to let you upload photos of cards."
        }
      ]
    ]
  }
}
